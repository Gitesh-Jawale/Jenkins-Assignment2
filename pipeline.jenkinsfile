pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                // Checkout the code from the repository
                git url: 'https://github.com/Gitesh-Jawale/emart'
            }
        }
        
        stage('Build') {
            steps {
                // Execute the Maven command within a shell
                sh 'mvn clean package'
            }
        }
    }
}
